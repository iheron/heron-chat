<!-- BEGIN BODY -->

<body class="page-500-full-page">

<div class="row-fluid">

    <div class="span12 page-500">

        <div class=" number">

            {{ error.status }}

        </div>

        <div class=" details">

            <h3>Opps, Something went wrong.</h3>

            <p>

                {{ message }}

            </p>

        </div>
        <div class="details">
            <pre>{{ error.stack }}</pre>
        </div>
    </div>

</div>
</body>

<!-- END BODY -->

